{
  "name": "Hausbot",
  "version": "0.0.1",
  "description": "Hogwarts but better bot",
  "main": "dist/bot.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "@discordjs/builders": "1.6.3",
    "axios": "1.4.0",
    "discord-api-types": "0.37.50",
