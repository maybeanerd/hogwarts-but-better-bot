{
  "name": "Hausbot",
  "version": "0.0.1",
  "description": "Hogwarts but better bot",
  "main": "dist/bot.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "@discordjs/builders": "1.7.0",
